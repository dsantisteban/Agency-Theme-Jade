    include views/navegador.jade 
    // Aside Baner
    include views/aside.jade
    // Servicios 
    include views/services.jade
    // Portafolio 
    include views/portafolio.jade
    // About
    include views/about.jade
    // Team
    include views/team.jade
    // Clientes
    include views/clients.jade
    // Contact
    include views/contact.jade
    // Footer
